// .eslintrc
{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
      
    },
    "plugins": ["@typescript-eslint"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }]
    },
    "env": {
      "browser": true,
      "es2021": true
    }
  }